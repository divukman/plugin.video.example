<?xml version="1.0" encoding="UTF-8"?>
<addon id="plugin.video.example"
version="2.1.0"
name="CatV (Cat TV)"
provider-name="Dimitar">
<requires>
  <import addon="xbmc.python" version="2.24.0"/>
</requires>
<extension point="xbmc.python.pluginsource" library="main.py">
  <provides>video</provides>
</extension>
<extension point="xbmc.addon.metadata">
  <summary lang="en">CatV (Cat TV)</summary>
  <description lang="en">Watch TV streams.</description>
  <disclaimer lang="en">Plugin for watching live TV streams.</disclaimer>
  <assets>
    <icon>icon.png</icon>
    <fanart>fanart.jpg</fanart>
    <screenshot>resources\screenshot-01.jpg</screenshot>
  </assets>
  <news>Updating the TV channels</news>
</extension>
</addon>
